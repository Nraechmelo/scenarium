{% extends 'layout.html.twig' %}
{% block title %}ajout des infos par le client{% endblock %}
{% block content %}

{{dump(table)}};
<form action="{{ url( '/client/table/{nom-de-la-table}') }}" method="POST" enctype="multipart/form-data">
			<div class="container">
				<div class="form-group row">
					<div class="col-4">
						<input type="text" name="name" class="form-control" placeholder="nom"/>
					</div>
					<div class="col-2">
						<input type="text" name="col" class="form-control" placeholder="colonne"/>
					</div>
					<div class="col-2">
						<input type="text" name="row" class="form-control" placeholder="ligne"/>
					</div>
					<div class="col-3">
						<input type="text" name="cplace" class="form-control" placeholder="nbre places"/>
					</div>
					<button class="btn btn-primary col-1">
						<i class="far fa-save"></i>

					</button>
				</div>

			</div>
		</form>

{% endblock %}